{
  "name": "nest-ravn-challenge",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npx dotenv -e .env.test -- jest --runInBand",
    "test:coverage": "npx dotenv -e .env.test -- jest --collect-coverage --runInBand",
    "test:categories": "npx dotenv -e .env.test -- jest categories  --runInBand",
    "test:categories:cov": "npx dotenv -e .env.test -- jest categories --collect-coverage --runInBand",
    "test:products:cov": "npx dotenv -e .env.test -- jest products --collect-coverage --runInBand",
    "test:tokens:cov": "npx dotenv -e .env.test -- jest tokens --collect-coverage --runInBand",
    "test:sendgrid:cov": "npx dotenv -e .env.test -- jest sendgrid --collect-coverage --runInBand",
    "test:likes:cov": "npx dotenv -e .env.test -- jest likes --collect-coverage --runInBand",
    "test:auth:cov": "npx dotenv -e .env.test -- jest auth --collect-coverage --runInBand",
    "test:orders:cov": "npx dotenv -e .env.test -- jest orders --collect-coverage --runInBand",
    "test:shopcarts:cov": "npx dotenv -e .env.test -- jest shopcarts --collect-coverage --runInBand",
    "test:items-in-cart:cov": "npx dotenv -e .env.test -- jest items-in-cart --collect-coverage --runInBand",
    "test:attachment:cov": "npx dotenv -e .env.test -- jest attachment --collect-coverage --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate:dev": "prisma migrate dev && prisma generate",
    "prisma:migrate:test": "npx dotenv -e .env.test -- prisma migrate dev && prisma generate",
    "prisma:studio:test": "npx dotenv -e .env.test -- prisma studio",
    "prisma:seed": "ts-node ./prisma/seed"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.4",
    "@prisma/client": "^3.3.0",
    "@sendgrid/mail": "^7.5.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.1",
    "aws-sdk": "^2.1016.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "nest-router": "^1.0.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^3.3.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "verbose": true,
    "collectCoverageFrom": [
      "*/services/*.ts",
      "**/utils/*.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
